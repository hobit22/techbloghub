# Terraform Variables Example
# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars is automatically ignored by git for security

# AWS Configuration
aws_region = "ap-northeast-2"

# Database Configuration
database_url      = "techbloghub"
database_username = "your_secure_admin_username"
database_password = "your_secure_db_password_here"

# OpenAI Configuration
openai_api_key = "your_openai_api_key_here"


# Admin
app_admin_username = "your_app_admin_username"
app_admin_password = "your_app_admin_password"

# RSS Proxy Configuration (for bypassing IP blocking)
rss_proxy_url     = "your_rss_proxy_url"

discord_webhook_url = "your_discord_webhook_url"