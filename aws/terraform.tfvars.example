# Terraform Variables Example
# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars is automatically ignored by git for security

# AWS Configuration
aws_region = "ap-northeast-2"

# Database Configuration
db_name     = "techbloghub"
db_username = "your_secure_admin_username"  # ì˜ˆ: techblog_admin_2024
db_password = "your_secure_db_password_here"

# OpenAI Configuration
openai_api_key = "your_openai_api_key_here"

# Examples:
# db_password = "MySecureP@ssw0rd123!"
# openai_api_key = "sk-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef"

# Admin 
app_admin_username = "your_app_admin_username"
app_admin_password = "your_app_admin_password"